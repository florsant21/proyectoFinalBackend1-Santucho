<div>
  <h1>Lista de Productos</h1>
  <ul id="productList">
    {{#each products}}
      <li>{{this.name}} - ${{this.price}}</li>
    {{/each}}
  </ul>

  <div>
    {{#if prevLink}}
      <a href="{{prevLink}}">Página anterior</a>
    {{/if}}
    <span>Página {{page}} de {{totalPages}}</span>
    {{#if nextLink}}
      <a href="{{nextLink}}">Página siguiente</a>
    {{/if}}
  </div>
</div>
